<!DOCTYPE html>
<html lang="it">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <!-- favicons -->
    <link rel="icon" href="<%= require('../favicons/output/favicon.ico') %>" sizes="32x32"/>
    <link rel="icon" href="<%= require('../favicons/output/favicon.svg') %>" type="image/svg+xml"/>
    <link rel="apple-touch-icon" href="<%= require('../favicons/output/apple-touch-icon.png') %>"/>
    <link rel="manifest" href="<%= require('../favicons/output/manifest.webmanifest') %>"/>

    <!-- alternativa (ma non processa il contenuto) -->
    <%= require('html-loader!./favicons/favicon.html').default %>

    <title><%= htmlWebpackPlugin.options.title %></title>
    <%= htmlWebpackPlugin.tags.headTags %> <!-- opzionale, param `inject: false` -->
  </head>
  <body>
    <div id="root" class="wrapper"></div>
    <%= htmlWebpackPlugin.tags.bodyTags %> <!-- opzionale, param `inject: false` -->
  </body>
</html>
